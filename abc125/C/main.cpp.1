int INF = 1 << 30;

int op(int a,int b){
    return gcd(a, b);
}

int end(){
    return (int)0;
}

int main(void){
    int n;
    cin >> n;
    vector<int> num(n);
    rep(i,n) cin >> num[i];
    segtree<int,op,end> tree(num);
    int ans = -INF;
    rep(i,n){
        int first,second;
        first = tree.prod(0,i);
        second = tree.prod(i+1,n);
        ans = max(ans,gcd(first,second));
    }
    cout << ans << endl;
}